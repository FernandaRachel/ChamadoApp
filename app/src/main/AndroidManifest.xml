<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.arqdsis.chamadoapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CriarChamados" />
        <activity android:name=".MenuActvity" />
        <activity android:name=".GerenciarFila" />
        <activity android:name=".ListaFila" />
        <activity android:name=".SpinnerActivity" />
        <activity
            android:name=".MenuNavigationActivity"
            android:label="@string/title_activity_menu_navigation"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>